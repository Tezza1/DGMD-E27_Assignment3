extends structure/_layout.pug

block content
    .uk-grid.uk-grid-divider
        .uk-width-medium-1-3
            article.uk-article.left-panel
                h1.uk-article-title Watch tutorials on YouTube
                p.uk-article-meta Last update 14 April 2017
                p.uk-article-lead Click on the thumbnail to watch some of the best tutorials on front-end frameworks. We have searched through all of the playlists to find the very best tutorials to help turn you into a professional front-end developer.
                hr.uk-article-divider
        .uk-width-medium-1-3
            .uk-thumbnail
                a(href="https://youtu.be/gqOEoUR5RHg")
                    img.uk-thumbnail-mini(src="img/bootstrap.png" alt="Bootstrap")
                    .uk-thumbnail-caption Bootstrap
            .uk-thumbnail
                a(href="https://youtu.be/2QcpR6cHpnk")
                    img.uk-thumbnail-mini(src="img/foundation.png" alt="")
                    .uk-thumbnail-caption Foundation
            .uk-thumbnail
                a(href="https://youtu.be/DIA4qJCTeYI")
                    img.uk-thumbnail-mini(src="img/jquery.png" alt="jQuery Mobile")
                    .uk-thumbnail-caption jQuery Mobile
            .uk-thumbnail
                a(href="https://youtu.be/XJxw_qQiGGs")
                    img.uk-thumbnail-mini(src="img/pure.png" alt="Yahoo Pure")
                    .uk-thumbnail-caption Pure
            .uk-thumbnail
                a(href="https://youtu.be/m9yEuYL5OiU")
                    img.uk-thumbnail-mini(src="img/semantic.png" alt="")
                    .uk-thumbnail-caption Semantic UI
            .uk-thumbnail
                a(href="https://youtu.be/JxplDTY65oY")
                    img.uk-thumbnail-mini(src="img/uikit.png" alt="Uikit")
                    .uk-thumbnail-caption UIkit
        .uk-width-medium-1-3
            .tm-article-subtitle.right-panel
                h3 Follow us on:
                .uk-block.uk-block-secondary.uk-contrast
                    article.uk-article
                        .uk-panel.icon
                            a(href="https://github.com/" class="uk-icon-hover uk-icon-medium uk-icon-github")  Github
                        .uk-panel.icon
                            a(href="https://twitter.com/?lang=en" class="uk-icon-hover uk-icon-medium uk-icon-twitter")  Twitter
                        .uk-panel.icon
                            a(href="https://www.facebook.com/" class="uk-icon-hover uk-icon-medium uk-icon-facebook-square")  Facebook
                        .uk-panel.icon
                            a(href="https://www.tumblr.com/" class="uk-icon-hover uk-icon-medium uk-icon-tumblr-square")  Tumblr
                        .uk-panel.icon
                            a(href="https://www.linkedin.com/uas/login" class="uk-icon-hover uk-icon-medium uk-icon-linkedin-square")  Linkedin
